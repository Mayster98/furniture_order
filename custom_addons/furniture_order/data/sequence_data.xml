<odoo>
    <record id="seq_furniture_product_serial" model="ir.sequence">
        <field name="name">Furniture Product Serial Number</field>
        <field name="code">furniture.product.serial</field>
        <field name="prefix">SN</field>
        <field name="padding">5</field>
    </record>

    <record id="seq_furniture_order_serial" model="ir.sequence">
        <field name="name">Furniture Order Serial Number</field>
        <field name="code">furniture.order.serial</field>
        <field name="prefix">ORD</field>
        <field name="padding">5</field>
    </record>
</odoo>